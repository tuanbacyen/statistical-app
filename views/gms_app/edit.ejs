<div class="col-md-8 offset-md-2 form-new">
  <h3>Update GMS-APP</h3>
  <form action="/gms_app/update" method="POST" autocomplete="off">
    <input type="hidden" name="_id" value="<%= gms_app._id %>">
    <div class="form-group">
      <label>Apk</label>
      <input type="text" class="form-control" name="apk" placeholder="Apk" value='<%= gms_app.apk %>' required></input>
    </div>
    <hr>
    <div class="form-group">
      <label>Packages</label>
      <input type="text" class="form-control" name="packages" placeholder="Packages" value='<%= gms_app.packages %>'
        required />
    </div>
    <div class="form-group">
      <label>Code Name</label>
      <input type="text" class="form-control" name="code_name" placeholder="Code Name" value='<%= gms_app.code_name %>'
        required />
    </div>
    <div class="form-group">
      <label>Code Version</label>
      <input type="text" class="form-control" name="code_version" placeholder="Code Version"
        value='<%= gms_app.code_version %>' required />
    </div>
    <div class="form-group">
      <a class="btn btn-secondary" href="/gms_app"><i class="fa fa-arrow-circle-left"></i>Back</a>
      <button type="submit" class="btn btn-success float-right"><i class="fa fa-database"></i> Submit</button>
    </div>
  </form>
</div>
