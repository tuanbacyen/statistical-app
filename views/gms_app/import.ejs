<div class="col-md-4 offset-md-4 form-new">
  <h3>Import file</h3>
  <form action="/gms_app/create_with_file" method="POST" autocomplete="off" enctype="multipart/form-data">
    <div class="form-group">
      <label for="file_gms_app">Example file input</label>
      <input type="file" class="form-control-file" name="ga_file">
    </div>
    <div class="form-group">
      <a class="btn btn-secondary" href="/gms_app"><i class="fa fa-arrow-circle-left"></i>Back</a>
      <button type="submit" class="btn btn-success float-right"><i class="fa fa-database"></i> Submit</button>
    </div>
  </form>
</div>
