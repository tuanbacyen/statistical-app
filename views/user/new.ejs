<div class="col-md-4 offset-md-4 form-new">
  <h3>New Country-Sale Check</h3>
  <form action="/user/create" method="POST" autocomplete="off">
    <div class="form-group">
      <label>Username</label>
      <input type="text" class="form-control" name="username" placeholder="username" value='' required></input>
    </div>
    <div class="form-group">
      <label>Password</label>
      <input type="text" class="form-control" name="password" placeholder="password" value='' required />
    </div>
    <div class="form-group">
      <a class="btn btn-secondary" href="/user"><i class="fa fa-arrow-circle-left"></i>Back</a>
      <button type="submit" class="btn btn-success float-right"><i class="fa fa-database"></i> Submit</button>
    </div>
  </form>
</div>
