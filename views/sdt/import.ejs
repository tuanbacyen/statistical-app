<div class="col-md-4 offset-md-4 form-new" style="background-color: #fff;margin-top: 25px;padding:20px;">
  <h3>Import file</h3>
  <form action="/sdt/create_with_file" method="POST" autocomplete="off" enctype="multipart/form-data">
    <div class="form-group">
      <label for="file_sdt">Example file input</label>
      <input type="file" class="form-control-file" id="file_sdt" name="sdt_file">
    </div>
    <div class="form-group">
      <a class="btn btn-secondary" href="/sdt"><i class="fa fa-arrow-circle-left"></i>Back</a>
      <button type="submit" class="btn btn-success float-right"><i class="fa fa-database"></i> Submit</button>
    </div>
  </form>
</div>
