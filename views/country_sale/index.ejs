<div class="col-md-8 offset-md-2">
  <h3>COUNTRY-SALE -> COUNT: <%= list.length %></h3>
  <table class="table table-striped table-bordered">
    <thead class="thead-dark">
      <tr class="d-flex">
        <th class="col-4">Country</th>
        <th class="col-4">Sale Code</th>
        <th class="col-4 text-center">
          <a class="btn btn-success" href="/country_sale/new"><i class="fa fa-plus"></i></a>
          <a class="btn btn-success" href="/country_sale/import"><i class="fa fa-upload"></i></a>
          <a href="/country_sale/delete" class="btn btn-success"
            onclick="return confirm('You want remove all record ?');"><i class="fa fa-trash-o"
              aria-hidden="true"></i></a>
      </tr>
    </thead>
    <tbody>
      <% list.forEach((country_sale) => { %>
      <tr class="d-flex">
        <td class="col-4"><%= country_sale.country %></td>
        <td class="col-4"><%= country_sale.sale_code %></td>
        <td class="col-4 text-center">
          <a href="/country_sale/edit/<%= country_sale._id %>"><i class="fa fa-pencil fa-lg" aria-hidden="true"></i></a>
          <a href="/country_sale/delete/<%= country_sale._id %>"
            onclick="return confirm('Are you sure to delete this record ?');"><i class="fa fa-trash fa-lg"
              aria-hidden="true"></i></a>
        </td>
      </tr>
      <% }); %>
    </tbody>
  </table>
</div>
