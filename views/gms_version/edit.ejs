<div class="col-md-4 offset-md-4 form-new">
  <h3>New gms version Check</h3>
  <form action="/gms_version/update" method="POST" autocomplete="off">
    <input type="hidden" name="_id" value="<%= gms_version._id %>">
    <div class="form-group">
      <label>version</label>
      <input type="text" class="form-control" name="version" placeholder="version" value="<%= gms_version.version %>"
        required />
    </div>
    <div class="form-group">
      <a class="btn btn-secondary" href="/gms_version"><i class="fa fa-arrow-circle-left"></i>Back</a>
      <button type="submit" class="btn btn-success float-right"><i class="fa fa-database"></i> Submit</button>
    </div>
  </form>
</div>
