<div class="col-md-6 offset-md-3">
  <h3>GMS-VERSION -> COUNT: <%= list.length %></h3>
  <table class="table table-striped table-bordered">
    <thead class="thead-dark">
      <tr class="d-flex">
        <th class="col-8">Gms Version</th>
        <th class="col-4 text-center">
          <a class="btn btn-success" href="/gms_version/new"><i class="fa fa-plus"></i></a>
          <a href="/gms_version/delete" class="btn btn-success"
            onclick="return confirm('You want remove all record ?');"><i class="fa fa-trash-o"
              aria-hidden="true"></i></a>
      </tr>
    </thead>
    <tbody>
      <% list.forEach((gms_version) => { %>
      <tr class="d-flex">
        <td class="col-8"><%= gms_version.version %></td>
        <td class="col-4 text-center">
          <a href="/gms_version/edit/<%= gms_version._id %>"><i class="fa fa-pencil fa-lg" aria-hidden="true"></i></a>
          <a href="/gms_version/delete/<%= gms_version._id %>"
            onclick="return confirm('Are you sure to delete this record ?');"><i class="fa fa-trash fa-lg"
              aria-hidden="true"></i></a>
        </td>
      </tr>
      <% }); %>
    </tbody>
  </table>
</div>
