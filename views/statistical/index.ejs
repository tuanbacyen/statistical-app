      </div>
      <div class="form-group">
        <label for="submission_id">Submission ID</label>
        <input type="text" class="form-control" id="submission_id" placeholder="6587245783416832">
      </div>
      <button class="btn btn-success" type="button" id="btn_reset">Reset</button>
      <button class="btn btn-primary" type="button" id="btn_statistical">Submit</button>
    </div>
    <div class="col-md-8 body-right">
      <div class="header-review">
        <nav class="navbar navbar-expand-lg navbar-light bg-light nav-tab">
          <button type="button" class="btn btn-outline-success btn-tab" id="tab_1" data-tab="1">SDT</button>
          <button type="button" class="btn btn-outline-success btn-tab" id="tab_2" data-tab="2">ISSUES</button>
        </nav>
      </div>
      <div class="body-review">
        <div class="table-review">
          <table class="table table-striped table-bordered" id="tbl_sdt">
