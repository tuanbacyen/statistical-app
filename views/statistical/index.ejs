<script>
  var GMS_VERSION = "<%= data_gv.version %>";
  var COUNTRY_SALE_CORRECT = [];
  var GMS_APP_CORRECT = [];
  var DATA = [];
</script>
<% cs_checks.forEach((cs) => { %>
<script>
  delete JSON.parse('<%- JSON.stringify(cs) %>')._id;
  COUNTRY_SALE_CORRECT.push(JSON.parse('<%- JSON.stringify(cs) %>'));
</script>
<% }); %>
<% ga_checks.forEach((ga) => { %>
<script>
  delete JSON.parse('<%- JSON.stringify(ga) %>')._id;
  GMS_APP_CORRECT.push(JSON.parse('<%- JSON.stringify(ga) %>'));
</script>
<% }); %>
<% data.forEach((d) => { %>
<script>
  DATA.push(JSON.parse('<%- JSON.stringify(d) %>'));
</script>
<% }); %>
<div class="container main-view">
  <% if(locals.flash) { %>
  <span class="badge badge-danger"><%= flash %></span>
  <% } %>
  <div class="row body-main">
    <div class="col-md-4 body-left">
      <form action="/check" method="POST" enctype="multipart/form-data">
        <div class="form-group">
          <label for="fileeee">Chọn folder cần check</label>
          <input type="file" class="form-control-file" id="fileeee" name="files_folder" required webkitdirectory
            mozdirectory>
        </div>
        <div class="form-group">
          <button class="btn btn-primary" type="submit">Submit</button>
        </div>
      </form>
    </div>
    <div class="col-md-8 body-right">
      <div class="header-review">
        <nav class="navbar navbar-expand-lg navbar-light bg-light nav-tab">
          <button type="button" class="btn btn-outline-success btn-tab" id="tab_1" data-tab="1">SDT</button>
          <button type="button" class="btn btn-outline-success btn-tab" id="tab_2" data-tab="2">ISSUES</button>
        </nav>
      </div>
      <div class="body-review">
        <div class="table-review">
          <table class="table table-striped table-bordered" id="tbl_sdt">
            <thead>
              <tr class="d-flex">
                <th class="col-1">STT</th>
                <th class="col-11">asd</th>
              </tr>
            </thead>
            <tbody id="tbl_body_sdt">
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>
<script src="/js/statistical.js"></script>
